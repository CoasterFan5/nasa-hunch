{
	"name": "nasa-hunch",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prisma": "prisma generate",
		"studio": "dotenv -e .env.test prisma studio",
		"db:deploy": "prisma migrate deploy",
		"seed": "tsx scripts/seed.ts",
		"s3devdata": "tsx scripts/s3.ts",
		"deploy": "pnpm install && prisma migrate deploy && prisma generate && pnpm run build"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.468.0",
		"@fontsource-variable/lexend": "^5.0.16",
		"@fontsource-variable/manrope": "^5.0.16",
		"@playwright/test": "^1.28.1",
		"@prisma/client": "^5.5.2",
		"@studio-freight/lenis": "^1.0.34",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/fluent-ffmpeg": "^2.1.23",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"fluent-ffmpeg": "^2.1.2",
		"libphonenumber-js": "^1.10.49",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.4.2",
		"sass": "^1.69.5",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"svelte-inview": "^4.0.2",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.4.1",
		"tsx": "^3.14.0",
		"typescript": "^5.2.2",
		"vite": "^5.0.0",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"@faker-js/faker": "^8.4.0",
		"@leodog896/bodyguard": "^1.3.2",
		"zod": "^3.22.4",
		"zod-validation-error": "^3.0.0"
	}
}