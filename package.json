		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.4.1",
		"tsx": "^3.14.0",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.32.2",
		"youtube-dl-exec": "^2.4.15"
